# Sepolia RPC URL - Get from Infura, Alchemy, or another RPC provider
VITE_RPC_URL=https://sepolia.infura.io/v3/YOUR_INFURA_KEY

# Deployed contract addresses (update after deployment)
VITE_TOKEN_ADDRESS=0xYourDeployedTokenAddress
VITE_FAUCET_ADDRESS=0xYourDeployedFaucetAddress

# Private key for deployment (with 0x prefix)
SEPOLIA_PRIVATE_KEY=0xYourPrivateKey

# Etherscan API key for contract verification
EVERIFY_API_KEY=YourEtherscanAPIKey
